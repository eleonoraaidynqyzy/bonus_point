
#include <iostream>
#include <string>

using namespace std;

int main() {
    string input;
    cin >> input; 

    bool isPalindrome = true;
    int len = input.length();

    for (int i = 0; i < len / 2; i++) {
        if (input[i] != input[len - 1 - i]) {
            isPalindrome = false;
            break;
        }
    }

    cout << (isPalindrome ? "yes" : "no") << endl; 
    return 0;
}
